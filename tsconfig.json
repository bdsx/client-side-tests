{
    "exclude": [
      "node_modules"
    ],
    "compilerOptions": {
      "target": "es2017",
      "lib": [ "es2017" ],
      "module": "CommonJS",
      "moduleResolution": "Node",
      "skipLibCheck": true,
      "alwaysStrict": true,
      "noImplicitAny": false,
      "strictNullChecks": true,
      "noImplicitThis": true,
      "resolveJsonModule": true,
      "importHelpers": true,
      "experimentalDecorators": true,
      "sourceMap": true
    }
  }
  